{
  "name": "ember-bunsen-core",
  "version": "7.0.1",
  "description": "Core bunsen library for Ember apps",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "lint-all-the-things",
    "start": "ember server",
    "test": "npm run lint && npm run test-addon",
    "test-addon": "EMBER_TRY_SCENARIO=${EMBER_TRY_SCENARIO:=ember-default} && ember try:one $EMBER_TRY_SCENARIO --- ember test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ciena-blueplanet/ember-bunsen-core.git"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "author": "Matthew Dahl (https://github.com/sandersky)",
  "license": "MIT",
  "devDependencies": {
    "bower": "^1.8.2",
    "broccoli-asset-rev": "^2.4.5",
    "chai-jquery": "^2.0.0",
    "ember-cli": "2.12.3",
    "ember-cli-chai": "0.4.3",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-htmlbars": "^1.1.1",
    "ember-cli-htmlbars-inline-precompile": "0.4.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-mocha": "0.14.4",
    "ember-cli-shims": "^1.0.2",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^1.0.5",
    "ember-hook": "1.4.2",
    "ember-load-initializers": "^1.1.0",
    "ember-resolver": "^5.0.1",
    "ember-sinon": "^0.7.0",
    "ember-source": "~2.12.0",
    "ember-test-utils": "^8.1.1",
    "loader.js": "^4.2.3",
    "sinon-chai": "^2.14.0"
  },
  "keywords": [
    "ember-addon",
    "bunsen",
    "bunsen-core",
    "core"
  ],
  "dependencies": {
    "broccoli-merge-trees": "^2.0.0",
    "bunsen-core": "^3.1.0",
    "ember-cli-babel": "^6.6.0",
    "ember-cli-moment-shim": "3.7.1",
    "ember-lodash-shim": "^4.0.0",
    "ember-seamless-immutable-shim": "^4.0.0",
    "ember-validator-shim": "^4.0.0",
    "ember-z-schema": "^5.0.1"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}